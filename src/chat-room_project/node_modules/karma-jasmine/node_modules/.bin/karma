#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules/karma/bin/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules/karma/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules/karma/bin/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules/karma/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/karma@6.4.4/node_modules:/mnt/c/Users/renau/Dev-document-tool/project/Code-cademy_Selection_project/src/chat-room_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../karma@6.4.4/node_modules/karma/bin/karma" "$@"
else
  exec node  "$basedir/../../../../../karma@6.4.4/node_modules/karma/bin/karma" "$@"
fi
